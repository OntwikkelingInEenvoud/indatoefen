<?xml version="1.0" encoding="utf-8"?>
<odoo>
      <data>
          <record id="oi1_free_worker.oi1_identification_data_wizard" model="ir.ui.view">
                <field name="name">oi1_free_worker.oi1_identification_data_wizard.oi1_identification_data_wizard</field>
                <field name="model">oi1_identification_data_wizard</field>
                <field name="arch" type="xml">
                    <form string="">
                        <group>
                             <field name="free_worker_id"  readonly="1"/>
                             <field name="ssn"  />
                             <field name="code" />
                             <field name="document_type_id" />
                             <field name="nationality_id"   options='{"no_create":true,"no_create_edit":true,"no_open":true}' />
                             <field name="expiration_date" />
                        </group>
                        <footer>
                                <button name="do_save_data" string="do_save_data" type="object" />
                                <button name="do_agree_save_data" string="do_agree_save_data" type="object" />
                                <button string="Cancel" class="oe_link" special="cancel"/>
                        </footer>
                    </form>
            </field>
          </record>
      </data>
</odoo>