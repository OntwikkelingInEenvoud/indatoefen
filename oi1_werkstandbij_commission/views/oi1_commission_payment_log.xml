<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="oi1_werkstandbij_commission.commission_payment_log_form" model="ir.ui.view">
            <field name="name">oi1_werkstandbij_commission_commission_payment_log_form</field>
            <field name="model">oi1_commission_payment_log</field>
            <field name="arch" type="xml">
                <form string="commission form">
                    <sheet>
                        <div class="oe_title oe_left">
                            <h1>
                                <group>
                                    <field name="name" readonly="1"/>
                                </group>
                            </h1>
                        </div>
                            <group>
                                  <group name="Payment" string="Payment" >
                                    <field name="partner_id" />
                                    <field name="role_id" />
                                    <field name="calculated_rate" />
                                    <field name="payment_by" />
                                  </group>
                                  <group name="Info"  string="Info">
                                    <field name="account_analytic_account_id" />
                                    <field name="commission_log_id"  />
                                    <field name="commission_payment_line_id" />
                                  </group>
                            </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>